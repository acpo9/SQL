SELECT singers.name, songs.song
FROM singers
JOIN songs
ON singers.id = songs.artist_id;

SELECT singers.name, COUNT(*) AS total_songs
FROM SINGERS
JOIN songs
ON singers.id = songs.artist_id
GROUP BY singers.name
ORDER BY total_songs DESC;
